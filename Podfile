# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'The Pray Together App' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for The Pray Together App

    pod 'Firebase'
    pod 'Firebase/Auth'
    pod 'Firebase/Database'
    pod 'FBSDKCoreKit'
    pod 'FBSDKShareKit'
    pod 'FBSDKLoginKit'
    pod 'Firebase/Storage'
    pod 'SDWebImage', '~> 4.0'
    pod 'SDWebImage/WebP'

end
